pulumi_content: |-
  #!/bin/sh
  npm install
  pulumi stack select dev
  pulumi up --yes
  pulumi stack output
  curl $(pulumi stack output url)
pulumi_git_path: aws-ts-ecs-anywhere
pulumi_uninstall: |-
  #!/bin/sh
  pulumi stack select dev
  pulumi refresh -y
  pulumi destroy --yes
  pulumi stack rm --yes
