deploy:
  type: text
  content: |+
    #/bin/sh
    pulumi config set aws:region us-west-1
    pulumi up --yes
    pulumi stack output --json
    curl $(pulumi stack output url)

test: []
uninstall:
  type: text
  content: |-
    #!/bin/sh

    pulumi destroy
    pulumi stack rm
dockerCompose: ''
