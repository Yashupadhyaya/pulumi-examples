pulumi_content: |
  #!/bin/sh
  pulumi stack select dev
  pulumi config set aws:region us-west-2
  pulumi up --yes
  pulumi stack output --json
  curl $(pulumi stack output url)
pulumi_git_path: aws-go-fargate/app
pulumi_uninstall: |-
  #!/bin/sh
  pulumi stack select dev
  pulumi refresh -y
  pulumi destroy --yes
  pulumi stack rm --yes
build:
  type: text
  content: |-
    #!/bin/sh

    # make all
