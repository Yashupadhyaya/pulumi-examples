pulumi_content: |-
  #!/bin/sh
  pulumi stack init dev
  pulumi config set aws:region ap-south-1
  pulumi up --yes
  pulumi stack output --json
  curl $(pulumi stack output url)
pulumi_git_path: aws-ts-ecs-anywhere
pulumi_uninstall: |-
  #!/bin/sh
  pulumi stack select dev
  pulumi refresh -y
  pulumi destroy --yes
  pulumi stack rm --yes
